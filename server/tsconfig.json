{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "lib": ["es6"],
    "allowJs": true,
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
